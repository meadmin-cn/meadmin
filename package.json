{
  "name": "project-name",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --build tsconfig.build.json && rimraf dist && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "pnpm build:packages && meadmin auto-import --watch --spawn \"pnpm exec nest start --watch\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "node test1.js && node test2.js",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "husky": "npx husky install",
    "build:packages": "tsc -b -v packages"
  },
  "dependencies": {
    "@meadmin/nest-swagger": "workspace:^1.0.0",
    "@nestjs/common": "^10.0.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.3",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.0.3",
    "@nestjs/swagger": "^7.1.1",
    "@nestjs/typeorm": "^10.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "iterare": "^1.2.1",
    "lodash": "^4.17.21",
    "mysql2": "^3.4.1",
    "nest-winston": "^1.9.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@meadmin/cli": "workspace:^1.0.0",
    "@nestjs/cli": "^10.0.3",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.3",
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "29.5.0",
    "lint-staged": "^13.2.2",
    "nest-commander": "^3.10.0",
    "prettier": "^2.8.8",
    "release-it": "^15.11.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.3"
  },
  "overrides": {
    "typeorm": {
      "glob": "^7.2.3"
    }
  },
  "lint-staged": {
    "src/**/*.ts": "prettier --cache --write --ignore-unknown",
    "{packages}/*/src/**/*.ts": [
      "prettier --cache --write --ignore-unknown",
      "eslint --cache --fix"
    ],
    "{src,apps,libs,test}/**/*.ts": "eslint --cache --fix"
  }
}
