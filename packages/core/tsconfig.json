{
  "compileOnSave": true,
  "compilerOptions": {
    "composite": true,
    "target": "es2018",
    "module": "commonjs",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "inlineSourceMap":false,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "stripInternal": true,
    "skipLibCheck": true,
    "noImplicitReturns": false,
    "pretty": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
    "baseUrl": "./src",
    "paths": {
      "@meadmin/*": ["./*"]
    },
    "typeRoots": [ "./types", "./node_modules/@types"]
  }
}
